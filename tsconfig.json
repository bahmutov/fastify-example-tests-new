{
  "compilerOptions": {
    "moduleResolution": "node",
    "types": ["cypress"],
    "target": "es2022",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["cypress/e2e/**/*.ts"]
}
